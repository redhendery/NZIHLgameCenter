{{#link-to 'schedule.admirals.new' tagName="li"}}<a href>Add New</a>{{/link-to}}

<div class="container-fluid">
  {{#each model as |scheduleAdmirals|}}
    <div class="col-md-6">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">{{scheduleAdmirals.game}} - {{scheduleAdmirals.home}} v {{scheduleAdmirals.away}}</h3>
        </div>
        <div class="panel-body">
          <span>
            Detais of the upcoming game
          </span>
        </div>
        <div class="panel-footer"><h6>
          <span>
            {{scheduleAdmirals.venue}}
          </span>
          <span class="pull-right">
            {{scheduleAdmirals.date}}
          </span></h6>
        </div>
      </div>
    </div>
  {{/each}}
</div>
