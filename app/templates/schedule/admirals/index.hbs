<div class="container-fluid">
  {{#each model as |scheduleAdmirals|}}
    <div class="col-md-6">
      <div class="panel panel-default">
        <div class="panel-heading">
          <div class="panel-title">
            <h4 class="text-center">{{scheduleAdmirals.teamGame}} - {{scheduleAdmirals.home}} v {{scheduleAdmirals.away}}</h4>
          </div>
        </div>
        <div class="panel-body">
          <span class="pull-left">
            <img class="home-logo" src="{{scheduleAdmirals.homeLogo}}" alt="{{scheduleAdmirals.home}}">
          </span>
          <span>
            Details of game
          </span>
          <span class="pull-right">
            <img class="away-logo" src="{{scheduleAdmirals.awayLogo}}" alt="{{scheduleAdmirals.away}}">
          </span>
        </div>
        <div class="panel-footer">
          <h6><span class="pull-left">
            {{scheduleAdmirals.venue}}
          </span>
          <span>
            Tickets
          </span>
          <span class="pull-right">
            {{scheduleAdmirals.date}}
          </span></h6>
        </div>
      </div>
    </div>
  {{/each}}
</div>
