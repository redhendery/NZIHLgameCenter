<div class="container-fluid">
  {{#each model as |scheduleReddevils|}}
    <div class="col-md-6">
      <div class="panel panel-default">
        <div class="panel-heading">
          <div class="panel-title">
            <h4 class="text-center">{{scheduleReddevils.teamGame}} - {{scheduleReddevils.home}} v {{scheduleReddevils.away}}</h4>
          </div>
        </div>
        <div class="panel-body">
          <span class="pull-left">
            <img class="home-logo" src="{{scheduleReddevils.homeLogo}}" alt="{{scheduleReddevils.home}}">
          </span>
          <span>
            Details of the upcoming game
          </span>
          <span class="pull-right">
            <img class="away-logo" src="{{scheduleReddevils.awayLogo}}" alt="{{scheduleReddevils.away}}">
          </span>
        </div>
        <div class="panel-footer">
          <h6><span class="pull-left">
            {{scheduleReddevils.venue}}
          </span>
          <span>
            Tickets
          </span>
          <span class="pull-right">
            {{scheduleReddevils.date}}
          </span></h6>
        </div>
      </div>
    </div>
  {{/each}}
</div>
